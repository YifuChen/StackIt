{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "camelcase": ["error", {"allow": ["^UNSAFE_"]}],
    "prefer-destructuring": ["error", {"object": false}]
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"]
}